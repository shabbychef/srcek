@article{RrWy1989,
author = {Radhakrishna Rao and Yuehua Wu}, 
title = {{A strongly consistent procedure for model selection in a regression problem}},
journal = {Biometrika},
volume = {76},
number = {2},
pages = {369-374},
doi = {10.1093/biomet/76.2.369},
year = {1989},
abstract = {We consider the multiple regression model Yn= Xn{beta}+ En, where Yn and En are n-vector random variables, Xn is an n<D7> m matrix and {beta} is an m-vector of unknown regression parameters. Each component of {beta} may be zero or nonzero, which gives ris e to 2m possible models for multiple regression. We provide a decision rule for the choice of a model which is strongly consistent fo r the true model as n [-&gt;]{infty}. The result is proved under certain mild conditions, for instance without assuming normality of the distribution of the components of En.},
URL = {http://biomet.oxfordjournals.org/cgi/content/abstract/76/2/369},
